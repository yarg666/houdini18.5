--HOUDINIMIMEBOUNDARY0xD3ADD339-0x00000F49-0x56B122C9-0x00000001HOUDINIMIMEBOUNDARY
Content-Disposition: attachment; filename="hdaroot/attr_LYNX_velocity_v.parm"
Content-Type: text/plain

{
version 0.8
folder0	[ 0	locks=0 ]	(	0	0	)
group	[ 0	locks=0 ]	(	""	)
grouptype	[ 0	locks=0 ]	(	"guess"	)
class	[ 0	locks=0 ]	(	"point"	)
vex_numcount	[ 0	locks=0 ]	(	10	)
vex_threadjobsize	[ 0	locks=0 ]	(	1024	)
snippet	[ 0	locks=0 ]	(	"string i = itoa(detail(1,\"ivalue\"));

// Ctrl Attr
float reverse_uv         = ch(\"../velocity_edge_reverse_uv_\"+i);

float rot_strength       = ch(\"../velocity_edge_rot_strength_\"+i);
float rot_offset         = ch(\"../velocity_edge_rot_master_\"+i+\"1\");
float rot_repeat         = ch(\"../velocity_edge_rot_master_\"+i+\"2\");
float rot_min            = ch(\"../velocity_edge_rot_master_\"+i+\"3\");
float rot_max            = ch(\"../velocity_edge_rot_master_\"+i+\"4\");
float rot_noise_offset   = ch(\"../velocity_edge_rot_noise_\"+i+\"1\");
float rot_noise_scale    = ch(\"../velocity_edge_rot_noise_\"+i+\"2\");
float rot_noise_min      = ch(\"../velocity_edge_rot_noise_\"+i+\"3\");
float rot_noise_max      = ch(\"../velocity_edge_rot_noise_\"+i+\"4\");

float scale_strength     = ch(\"../velocity_edge_scale_strength_\"+i);
float scale_offset       = ch(\"../velocity_edge_scale_master_\"+i+\"1\");
float scale_repeat       = ch(\"../velocity_edge_scale_master_\"+i+\"2\");
float scale_min          = ch(\"../velocity_edge_scale_master_\"+i+\"3\");
float scale_max          = ch(\"../velocity_edge_scale_master_\"+i+\"4\");
float scale_noise_offset = ch(\"../velocity_edge_scale_noise_\"+i+\"1\");
float scale_noise_scale  = ch(\"../velocity_edge_scale_noise_\"+i+\"2\");
float scale_noise_min    = ch(\"../velocity_edge_scale_noise_\"+i+\"3\");
float scale_noise_max    = ch(\"../velocity_edge_scale_noise_\"+i+\"4\");

// Attr
vector @LYNX_velocity_uv;
vector @LYNX_velocity_N;
vector @LYNX_velocity_tan_u;
vector @LYNX_velocity_v;

vector LYNX_velocity_v = @LYNX_velocity_N;
float uv_u = @LYNX_velocity_uv[0]; if (reverse_uv==1) {uv_u=1-uv_u;}
matrix rot_mat;
/// Rotate
float rot_uv_u = (rot_offset+(uv_u * rot_repeat*.999999))%1;
float rot_remap_se = chramp(\"../velocity_edge_rot_se_ramp_\"+i,uv_u);
float rot_remap = chramp(\"../velocity_edge_rot_ramp_\"+i,rot_uv_u);
float rot_noise = fit01(noise((uv_u+rot_noise_offset)*rot_noise_scale),rot_noise_min,rot_noise_max);
float rot_angle = rot_remap_se * (rot_strength + (rot_noise + (fit01(rot_remap,rot_min,rot_max))));
rot_mat = qconvert(quaternion(radians(rot_angle),@LYNX_velocity_tan_u));

/// Scale
float scale_uv_u = (scale_offset+(uv_u * scale_repeat*.999999))%1;
float scale_remap_se = chramp(\"../velocity_edge_scale_se_ramp_\"+i,uv_u);
float scale_remap = chramp(\"../velocity_edge_scale_ramp_\"+i,scale_uv_u);
float scale_noise = fit01(noise((uv_u+scale_noise_offset)*scale_noise_scale),scale_noise_min,scale_noise_max);
float scale_value = scale_remap_se * (scale_strength * (scale_noise * (fit01(scale_remap,scale_min,scale_max))));

LYNX_velocity_v = (LYNX_velocity_v * rot_mat) * scale_value;
@LYNX_velocity_v = LYNX_velocity_v;


"	)
exportlist	[ 0	locks=0 ]	(	*	)
vex_strict	[ 0	locks=0 ]	(	"off"	)
autobind	[ 0	locks=0 ]	(	"on"	)
bindings	[ 0	locks=0 ]	(	0	)
