snippet	[ 0	locks=0 ]	(	"int prim_pts[] = primpoints(0,@primnum);
int LYNX_fabric_resample_segment_pts[];
int LYNX_fabric_resample_segment_idx = 0;
float LYNX_fabric_resample_segment_twists_min[] = {0};
float LYNX_fabric_resample_segment_twists_max[] = {0};
float LYNX_fabric_resample_segment_length[] = {0};
int prim_id = 0;
int LYNX_fabric_resample = 0;
for(int i=0;i<len(prim_pts)-1;i++)
{   
    int pt_id = prim_pts[i];
    vector pt_P = point(0,\"P\",pt_id); 
    float pt_LYNX_fabric_thread_twists = point(0,\"LYNX_fabric_thread_twists\",pt_id);
    int pt_LYNX_fabric_resample = inpointgroup(0,\"LYNX_fabric_resample\",pt_id); 
    int pt_n_id = prim_pts[i+1];
    vector pt_n_P = point(0,\"P\",pt_n_id);
    float pt_n_LYNX_fabric_thread_twists = point(0,\"LYNX_fabric_thread_twists\",pt_n_id);
    int pt_n_LYNX_fabric_resample = inpointgroup(0,\"LYNX_fabric_resample\",pt_n_id);
    float pt_distance = distance(pt_P,pt_n_P);
    append(LYNX_fabric_resample_segment_pts,pt_id);
    if (i==0){
        LYNX_fabric_resample = pt_LYNX_fabric_resample;
        LYNX_fabric_resample_segment_twists_min[LYNX_fabric_resample_segment_idx] = pt_LYNX_fabric_thread_twists; 
        LYNX_fabric_resample_segment_twists_max[LYNX_fabric_resample_segment_idx] = pt_LYNX_fabric_thread_twists; 
    }
    if(LYNX_fabric_resample==0 && pt_n_LYNX_fabric_resample!=pt_LYNX_fabric_resample){
        append(LYNX_fabric_resample_segment_pts,-1);
        LYNX_fabric_resample = 1;
        LYNX_fabric_resample_segment_length[LYNX_fabric_resample_segment_idx] += pt_distance; 
        LYNX_fabric_resample_segment_idx += 1;         
        append(LYNX_fabric_resample_segment_length,0);
        append(LYNX_fabric_resample_segment_twists_min,pt_n_LYNX_fabric_thread_twists);
        append(LYNX_fabric_resample_segment_twists_max,pt_n_LYNX_fabric_thread_twists);
        continue;
    }
    if(LYNX_fabric_resample==1){
        LYNX_fabric_resample_segment_twists_min[LYNX_fabric_resample_segment_idx] = min(pt_LYNX_fabric_thread_twists,min(pt_n_LYNX_fabric_thread_twists,LYNX_fabric_resample_segment_twists_min[LYNX_fabric_resample_segment_idx])); 
        LYNX_fabric_resample_segment_twists_max[LYNX_fabric_resample_segment_idx] = max(pt_LYNX_fabric_thread_twists,max(pt_n_LYNX_fabric_thread_twists,LYNX_fabric_resample_segment_twists_max[LYNX_fabric_resample_segment_idx])); 
    }
    if(LYNX_fabric_resample==1 && pt_n_LYNX_fabric_resample!=pt_LYNX_fabric_resample){
        append(LYNX_fabric_resample_segment_pts,-1);
        LYNX_fabric_resample = 0;
        LYNX_fabric_resample_segment_idx += 1; 
        append(LYNX_fabric_resample_segment_length,0);
        append(LYNX_fabric_resample_segment_twists_min,0);
        append(LYNX_fabric_resample_segment_twists_max,0);
        LYNX_fabric_resample_segment_length[LYNX_fabric_resample_segment_idx] += pt_distance; 
        continue;
    }
    LYNX_fabric_resample_segment_length[LYNX_fabric_resample_segment_idx] += pt_distance; 
}
append(LYNX_fabric_resample_segment_pts,prim_pts[-1]);
LYNX_fabric_resample_segment_idx = 0;
for(int i=0;i<len(LYNX_fabric_resample_segment_pts);i++)
{   
    int pt_id = LYNX_fabric_resample_segment_pts[i];
    if (pt_id==-1){
        LYNX_fabric_resample_segment_idx += 1;
    };
    float segment_length = LYNX_fabric_resample_segment_length[LYNX_fabric_resample_segment_idx] / ((ceil(LYNX_fabric_resample_segment_twists_max[LYNX_fabric_resample_segment_idx]-LYNX_fabric_resample_segment_twists_min[LYNX_fabric_resample_segment_idx])/360)*ch(\"../fabric_fiber_resample_segments\"));
    setpointattrib(0,\"segment_length\",pt_id,segment_length,\"set\");
}"	)
exportlist	[ 0	locks=0 ]	(	*	)
vex_strict	[ 0	locks=0 ]	(	"off"	)
autobind	[ 0	locks=0 ]	(	"on"	)
bindings	[ 0	locks=0 ]	(	0	)
