{
version 0.8
folder0	[ 0	locks=0 ]	(	0	0	)
group	[ 0	locks=0 ]	(	""	)
grouptype	[ 0	locks=0 ]	(	"guess"	)
class	[ 0	locks=0 ]	(	"primitive"	)
vex_numcount	[ 0	locks=0 ]	(	10	)
vex_threadjobsize	[ 0	locks=0 ]	(	1024	)
snippet	[ 0	locks=0 ]	(	"int prim_pts[] = primpoints(0, @primnum);
int prim_pts_count = len(prim_pts);

vector tan_u_list_first = normalize(point(0, \"P\", prim_pts[1]) - point(0, \"P\", prim_pts[0]));
vector N_first = normalize(haspointattrib(0,\"N\")?point(0,\"N\",prim_pts[0]):set(0,1,0));
N_first = normalize(cross(tan_u_list_first, cross(N_first, tan_u_list_first)));

// forward declarations
vector tan_v_list;
vector tan_u_list[];
vector N_list[];
float theta = 0;

// fill arrays
for (int i = 0; i<prim_pts_count-1; i++)
    {
    push(tan_u_list, normalize(point(0, \"P\", prim_pts[i+1]) - point(0, \"P\", prim_pts[i])));
    push(N_list, N_first);
    }
push(tan_u_list, tan_u_list[prim_pts_count-2]);
push(N_list, N_list[prim_pts_count-2]);

// parallel transport
for (int i = 0; i<prim_pts_count-1; i++)
    {
    tan_v_list = cross(tan_u_list[i],tan_u_list[i+1]);


    if (length(tan_v_list) == 0)
        {
        N_list[i+1] = N_list[i];
        }
    else
        {
        tan_v_list = normalize(tan_v_list);
        theta = acos(dot(tan_u_list[i], tan_u_list[i+1]));
        matrix rotmat = ident(); 
        rotate(rotmat, theta, tan_v_list); 
        N_list[i+1] = rotmat * N_list[i];
        }
    }
    
// set attributes
for (int j = 0; j < prim_pts_count; j++)
    {
    setpointattrib(0, \"LYNX_fabric_N\", prim_pts[j], N_list[j], \"set\");
}"	)
