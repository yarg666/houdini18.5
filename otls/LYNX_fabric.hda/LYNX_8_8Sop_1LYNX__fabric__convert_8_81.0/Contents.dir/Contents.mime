    push(N_list, N_first);
    }
push(tan_u_list, tan_u_list[prim_pts_count-2]);
push(N_list, N_list[prim_pts_count-2]);

// parallel transport
for (int i = 0; i<prim_pts_count-1; i++)
    {
    tan_v_list = cross(tan_u_list[i],tan_u_list[i+1]);


    if (length(tan_v_list) == 0)
        {
        N_list[i+1] = N_list[i];
        }
    else
        {
        tan_v_list = normalize(tan_v_list);
        theta = acos(dot(tan_u_list[i], tan_u_list[i+1]));
        matrix rotmat = ident(); 
        rotate(rotmat, theta, tan_v_list); 
        N_list[i+1] = rotmat * N_list[i];
        }
    }
    
// set attributes
for (int j = 0; j < prim_pts_count; j++)
    {
    setpointattrib(0, \"LYNX_fabric_N\", prim_pts[j], N_list[j], \"set\");
}"	)
