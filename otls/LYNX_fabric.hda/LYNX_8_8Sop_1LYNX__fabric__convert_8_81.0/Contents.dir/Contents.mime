snippet	[ 0	locks=0 ]	(	"int prim_pts[] = primpoints(0, @primnum);
int prim_pts_count = len(prim_pts);

vector tan_u_list_first = normalize(point(0, \"P\", prim_pts[1]) - point(0, \"P\", prim_pts[0]));
vector N_first = normalize(haspointattrib(0,\"N\")?point(0,\"N\",prim_pts[0]):set(0,1,0));
